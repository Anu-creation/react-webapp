      </div>\r\n                <div className=\"channel-name\">{channel} Channel</div>\r\n                <ColorContext.Consumer>\r\n            {([themeColor]) => (\r\n              <button\r\n                 onClick={this.toggleModal}\r\n                 style={{ backgroundColor: themeColor }}\r\n                >\r\n                Watch on Youtube\r\n              </button>\r\n            )}\r\n          </ColorContext.Consumer>\r\n                <p>{description}</p>\r\n\r\n                {showModal ? (\r\n            <Modal>\r\n              <div>\r\n                <h1>Would you like to watch this video on YouTube ?</h1>\r\n                <div className=\"buttons\">\r\n                  <button className=\"btn-green\" onClick={this.goToYoutube}>\r\n                    Yes\r\n                    </button>\r\n                  <button onClick={this.toggleModal}>No</button>\r\n                </div>\r\n              </div>\r\n            </Modal>\r\n          ) : null}\r\n              </div>\r\n            </div>\r\n          );\r\n    }\r\n}\r\n\r\nexport default function WatchAreaWithErrorBoundary(props) {\r\n  return (\r\n    <ErrorBoundary>\r\n      <WatchArea {...props} />\r\n    </ErrorBoundary>\r\n  );\r\n}"},"lineCount":null}},"error":null,"hash":"7e5ec6a19d551c74967f08b492e76c9a","cacheData":{"env":{}}}